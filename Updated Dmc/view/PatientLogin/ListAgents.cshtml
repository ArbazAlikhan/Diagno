@{
    ViewBag.Title = "ListAgents";
}

<h3 style="color:darkblue;text-align:center">Details of the Agent</h3>
<table class="table" style="margin-top:30px;">
    <thead>
        <tr>
            <th>Agent Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age </th>
            <th>Gender</th>
            <th>Contact Number</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@ViewBag.AgentId</td>
            <td>@ViewBag.FirstName</td>
            <td>@ViewBag.LastName</td>
            <td>@ViewBag.Age</td>
            <td>@ViewBag.Gender</td>
            <td>@ViewBag.ContactNumber</td>
        </tr>
    </tbody>


</table>
