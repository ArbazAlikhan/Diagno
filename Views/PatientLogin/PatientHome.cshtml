        <a href='@Url.Action("Appointmentview","AgentLogin")' class="btn btn-info btn-group-lg">View Appointment</a>
        <br />
        <a href='@Url.Action("", "DoctorLogin")' class="btn btn-primary btn-group-lg ">View TestResults</a>
        <br />
        <a href="" class="btn btn-warning btn-group-lg">Logout</a>
    </div>
   
}

